<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder' xmlns:g="urn:import:com.google.gwt.user.client.ui"
             xmlns:gxt="urn:import:com.sencha.gxt.widget.core.client" xmlns:container="urn:import:com.sencha.gxt.widget.core.client.container">

    <ui:with type="java.lang.String" field="string_label" />
    <ui:with type="com.sencha.gxt.widget.core.client.ContentPanel.ContentPanelAppearance" field="contentPanelAppearance" />
    <ui:with type="com.sencha.gxt.widget.core.client.container.AccordionLayoutContainer.AccordionLayoutAppearance"
             field="accordionLayoutAppearance" />


    <gxt:ContentPanel ui:field="panel" appearance="{contentPanelAppearance}" headerVisible="false"
                      bodyBorder="false" pixelSize="200, 325" addStyleNames="margin-10">
        <container:AccordionLayoutContainer ui:field="con" expandMode="SINGLE_FILL">

            <gxt:ContentPanel ui:field="users" appearance="{accordionLayoutAppearance}" animCollapse="true"
                              headingText="Lab1">

                <g:Label text="{string_label}" />
            </gxt:ContentPanel>

        </container:AccordionLayoutContainer>
    </gxt:ContentPanel>

</ui:UiBinder>